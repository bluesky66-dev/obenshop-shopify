            <div class="bg-effect-{{block.id }}" style="position:relative;  ">
              <div class="ovrly17">
                {% if section.settings.link_1_grid != blank %}<a href="{{ section.settings.link_1_grid }}">{% endif%}
                {% if section.settings.image_1_grid != blank %}
                <img src="{{ section.settings.image_1_grid | img_url: 'master' }}" alt="{{ section.settings.title_1_grid }}" />
                {% else %}
                <img src="//via.placeholder.com/800x800">
                {% endif%}   
                {% if section.settings.overlay_1_grid %}  <div class="ovrly" style="background:{{ section.settings.overlay_bgcolor_1_grid | color_modify: 'alpha', 0.7 }};"></div> {% endif %}      
                <div class="ovrly">   
                </div>
                {% if section.settings.link_1_grid != blank %}  </a>        
                {% endif%}
              </div>
              <div class="featured-content">              
                {% if section.settings.title_1_grid != blank %}   
                <h2 {% if section.settings.overlay_1_grid %} style="background:{{ section.settings.title_bg_color_1_grid | color_modify: 'alpha', 0.8 }}" {% endif %}>{% if section.settings.link_1_grid != blank %}<a href="{{ section.settings.link_1_grid }}">{% endif%}{{ section.settings.title_1_grid }}{% if section.settings.link_1_grid != blank %}</a>{% endif%}</h2>
                {% endif %} 
              </div>   
            </div>
          </div> 





          <div class="grid__item wide--one-third post-large--one-third large--one-third medium--one-half small-grid__item wow fadeInUp animated">
            <div class="bg-effect-{{block.id }}" style="position:relative;  ">
              <div class="ovrly17">
                {% if section.settings.link_2_grid != blank %}<a href="{{ section.settings.link_2_grid }}">{% endif%}
                {% if section.settings.image_2_grid != blank %}
                <img src="{{ section.settings.image_2_grid | img_url: 'master' }}" alt="{{ section.settings.title_2_grid }}" />
                {% else %}
                <img src="//via.placeholder.com/800x800">
                {% endif%}   
                {% if section.settings.overlay_2_grid %}  <div class="ovrly" style="background:{{ section.settings.overlay_bgcolor_2_grid | color_modify: 'alpha', 0.7 }};"></div> {% endif %}      
                <div class="ovrly">   
                </div>
                {% if section.settings.link_2_grid != blank %}  </a>        
                {% endif%}
              </div>
              <div class="featured-content">              
                {% if section.settings.title_2_grid != blank %}   
                <h2 {% if section.settings.overlay_2_grid %} style="background:{{ section.settings.title_bg_color_2_grid | color_modify: 'alpha', 0.8 }}" {% endif %}>{% if section.settings.link_2_grid != blank %}<a href="{{ section.settings.link_2_grid }}">{% endif%}{{ section.settings.title_2_grid }}{% if section.settings.link_2_grid != blank %}</a>{% endif%}</h2>
                {% endif %} 
              </div>   
            </div>
          </div> 





          <div class="grid__item wide--one-third post-large--one-third large--one-third medium--one-half small-grid__item wow fadeInUp animated">
            <div class="bg-effect-{{block.id }}" style="position:relative;  ">
              <div class="ovrly17">
                {% if section.settings.link_3_grid != blank %}<a href="{{ section.settings.link_3_grid }}">{% endif%}
                {% if section.settings.image_3_grid != blank %}
