                {% endif%}   
                {% if section.settings.overlay_3_grid %}  <div class="ovrly" style="background:{{ section.settings.overlay_bgcolor_3_grid | color_modify: 'alpha', 0.7 }};"></div> {% endif %}      
                <div class="ovrly">   
                </div>
                {% if section.settings.link_3_grid != blank %}  </a>        
                {% endif%}
              </div>
              <div class="featured-content">              
                {% if section.settings.title_3_grid != blank %}   
                <h2 {% if section.settings.overlay_3_grid %} style="background:{{ section.settings.title_bg_color_3_grid | color_modify: 'alpha', 0.8 }}" {% endif %}>{% if section.settings.link_3_grid != blank %}<a href="{{ section.settings.link_3_grid }}">{% endif%}{{ section.settings.title_3_grid }}{% if section.settings.link_3_grid != blank %}</a>{% endif%}</h2>
