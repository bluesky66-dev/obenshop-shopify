    <div class="container">
      {% if section.settings.section_2_mainheading != blank %}
      <div class="border-title">       
        <h2>  {{section.settings.section_2_mainheading}}</h2>      
        {% if section.settings.section_2_subheading != blank %}
        <h6>  {{section.settings.section_2_subheading}}</h6>
        {% endif %}
        {% if  section.settings.section_4_img  != blank %}
        <img src="{{  section.settings.section_4_img  | img_url: "master" }}" alt="{{ section.settings.section_4_imgheading }}" />
        {% endif %}
      </div>
      {% endif %}      
      <div class="grid__item">
        <div class="grid__item wide--one-half post-large--one-half large--one-half medium--grid__item small--grid__item icon-left">
          <div class="icon-wrapper ">
            <div class="icon-detail">
              {% if section.settings.section_2_iconheading_one != blank %}
              <h4>  {{section.settings.section_2_iconheading_one}}</h4>
              {% endif %}
              {% if section.settings.section_2_icondetail_one != blank %}
              {{section.settings.section_2_icondetail_one}}
              {% endif %}
            </div>
            <div class="icon">
              {% if section.settings.section_2_iconcls_one != blank %}
              <span class="{{section.settings.section_2_iconcls_one}}"> </span>
              {% endif %}
            </div>
          </div>
          <div class="icon-wrapper ">
            <div class="icon-detail">
              {% if section.settings.section_2_iconheading_two != blank %}
              <h4>  {{section.settings.section_2_iconheading_two}}</h4>
              {% endif %}
              {% if section.settings.section_2_icondetail_two != blank %}
              {{section.settings.section_2_icondetail_two}}
              {% endif %}
            </div>
            <div class="icon">
              {% if section.settings.section_2_iconcls_two != blank %}
              <span class="{{section.settings.section_2_iconcls_two}}"> </span>
              {% endif %}
            </div>
          </div>
        </div>

        <div class="grid__item wide--one-half post-large--one-half large--one-half medium--grid__item small--grid__item icon-right">
          <div class="icon-wrapper ">
            <div class="icon">
              {% if section.settings.section_2_iconcls_three != blank %}
              <span class="{{section.settings.section_2_iconcls_three}}"> </span>
              {% endif %}
            </div>
            <div class="icon-detail">
              {% if section.settings.section_2_iconheading_three != blank %}
              <h4>  {{section.settings.section_2_iconheading_three}}</h4>
              {% endif %}
              {% if section.settings.section_2_icondetail_three != blank %}
              {{section.settings.section_2_icondetail_three}}
              {% endif %}
            </div>
          </div>
          <div class="icon-wrapper ">
            <div class="icon">
              {% if section.settings.section_2_iconcls_four != blank %}
              <span class="{{section.settings.section_2_iconcls_four}}"> </span>
              {% endif %}
            </div>
            <div class="icon-detail">
              {% if section.settings.section_2_iconheading_four != blank %}
              <h4>  {{section.settings.section_2_iconheading_four}}</h4>
              {% endif %}

              {% if section.settings.section_2_icondetail_four != blank %}
              {{section.settings.section_2_icondetail_four}}
              {% endif %}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="dt-sc-hr-invisible-large"></div> 
  {% endif %}




  {% if section.settings.show_3_section != blank %}
  <div class="grid-uniform section-four" style=""; >
    <div class="container">
      {% if section.settings.section_4_maintitle != blank %}
      <div class="main-title">
        <h2>  {{section.settings.section_4_maintitle }}</h2>

      </div>
      {% endif %}
      <div class="grid__item ">
        <div class="grid__item wide--two-thirds post-large--two-thirds large--two-thirds">
          {% if section.settings.section_3_desc != blank %}
          {{section.settings.section_3_desc }}
          <div class="invisible-medium"></div>
          {% endif %}
          <div class="grid__item ">
            {% if section.settings.section_3_imageone != blank %}
            <div class="grid__item wide--one-third post-large--one-third large--one-third medium--one-half small--grid__item">
              <img src="{{ section.settings.section_3_imageone | img_url: '1024x1024' }}" alt="" title="" /> 
            </div>
            {% endif %}
            {% if section.settings.section_3_imagetwo != blank %}
            <div class="grid__item wide--one-third post-large--one-third large--one-third medium--one-half small--grid__item">
              <img src="{{ section.settings.section_3_imagetwo | img_url: '1024x1024' }}" alt="" title="" /> 
            </div>
            {% endif %}
            {% if section.settings.section_3_imagethree != blank %}
            <div class="grid__item wide--one-third post-large--one-third large--one-third medium--one-half small--grid__item">
              <img src="{{ section.settings.section_3_imagethree | img_url: '1024x1024' }}" alt="" title="" /> 
            </div>
            {% endif %}
          </div>
        </div>   
        {% if section.settings.section_3_rightimage != blank %}
        <div class="grid__item wide--one-third post-large--one-third large--one-third">
          <img src="{{ section.settings.section_3_rightimage | img_url: '1024x1024' }}" alt="" title="" /> 
        </div>
        {% endif %}
      </div>
    </div>
  </div>
  <div class="dt-sc-hr-invisible-large"></div> 
  {% endif %}


  {% if section.settings.section_three %}
  <div class="grid-uniform section-three " style="background:{{section.settings.team_section_bg}};float:left;width:100%;">
    <div class="container">
      {% if  section.settings.section_3_title  != blank %}
      <div class="border-title">
        <h2 style="color:{{section.settings.section_3_title_color  }};background:{{section.settings.section_3_titlebg_color}}">{{section.settings.section_3_title}}</h2>
        {% if  section.settings. section_3_subtitle  != blank %}
        <h4 style="color:{{section.settings.section_3_subtitle_color}}">{{section.settings.section_3_subtitle}}</h4>
        {% endif %}

      </div>
      {% endif %}
      <div class="grid__item">

        {% case section.blocks.size %}
        {% when 1 %}
        {%- assign block_width = 'one-whole text-center' -%}
        {% when 2 %}
        {%- assign block_width = 'wide--one-half post-large--one-half large--one-half medium--one-half small-grid__item' -%}
        {% when 3 %}
        {%- assign block_width = 'wide--one-third post-large--one-third large--one-third medium--one-half small-grid__item' -%}
        {% when 4 %}
        {%- assign block_width = 'wide--one-quarter post-large--one-quarter large--one-quarter medium--one-half small-grid__item' -%}
        {% endcase %}

        {% if section.blocks.size > 0 %}      
        {% for block in section.blocks %}

        <div class="grid__item {{ block_width }} ">
          <div class="team">
            {% if  block.settings.team_1_img   != blank %}
            <div class="team-img">
              <img src="{{ block.settings.team_1_img   | img_url: "master" }}" alt="{{ block.settings.team_1_imgheading }}" />
            </div>
            {% endif %}
            <div class="team-detail">
              {% if  block.settings.team_1_title  != blank %}
              <h4>{{ block.settings.team_1_title }}</h4>
              {% endif %}
              {% if  block.settings.team_1_designation  != blank %}
              <h5>{{ block.settings.team_1_designation }}</h5>
              {% endif %}
            </div>
          </div>
        </div>

        {% endfor %}
        {% endif %}







      </div>
    </div>
  </div>
  {% endif %} 

</div>




























<style>

  .grid-banner-type-18  h2 a,.grid-banner-type-5  h2  { color:{{ section.settings.title_color_1_grid }};text-align:center; }
  .grid-banner-type-18  h2 a:hover  { color:{{ section.settings.title_hover_color_1_grid }}; }



  .grid-banner-type-18 .collectionItems .ovrly17::after{background:{{section.settings.overlay_bg_color}};}

  .grid-banner-type-18 .grid__item:hover h2 {background:{{section.settings.texthover_bg_color}}!important;color:{{section.settings.texthover_txt_color}}!important;}

  .grid-banner-type-18 .grid__item:hover h2 a{color:{{section.settings.texthover_txt_color}}!important;}


  .aboutus .section-two{background:{{section.settings.section_2_bgcolor}};}

  .aboutus .section-two .border-title h2 ,  .aboutus .section-two .border-title h6{color:{{section.settings.section_2_txtcolor}};}

  .aboutus .icon-wrapper  h4 , .aboutus .icon-wrapper  p{color:{{section.settings.section_2_txtcolor}};}

  .aboutus .icon{background:{{section.settings.section_2_txtcolor}};}

  .aboutus .icon-wrapper  h4 , .vedabout-seven .icon-wrapper  p{color:{{section.settings.section_2_txtcolor}};}

  .aboutus .icon-wrapper span {color:{{section.settings.section_2_iconcolor}};}

  .aboutus .icon-wrapper:hover .icon{background:{{section.settings.section_2_iconcolor}};}

  .aboutus .icon-wrapper:hover .icon span{color:{{section.settings.section_2_txtcolor}};}



  .aboutus .section-four p{color:{{section.settings.section_1_desccolor}};}
  .aboutus .team-img { box-shadow: 0 0 0 10px {{section.settings.team_4_bordercolor}}; }

  .aboutus .team-detail h4 { color:{{section.settings.team_4_authorcolor}};}

  .aboutus .team-detail h5{color:{{section.settings.team_4_designationcolor}};}

  .aboutus .team:hover .team-img {box-shadow: 0 0 0 10px {{section.settings.team_4_authorcolor}};}

  .aboutus .block-three img{border: 10px solid {{section.settings.img_bordercolor}};}


</style>













