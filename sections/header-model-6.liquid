  
 
          
              <div class="menu_icon_section">
            <div class="header_top">
              {% if template == 'index' %}
              <h1 class="grid__item wide--two-sixths post-large--two-sixths large--two-sixths site-header__logo" itemscope itemtype="http://schema.org/Organization">
                {% else %}
                <div class="h1 grid__item wide--two-sixths post-large--two-sixths large--two-sixths site-header__logo" itemscope itemtype="http://schema.org/Organization">
                  {% endif %}
                  {% if settings.logo_use_image %}
                  <a href="/" style="max-width: {{ settings.logo_max_width }}px;">
                    <img class="normal-logo" src="{{ 'logo.png' | asset_url }}" alt="{{ shop.name }}" itemprop="logo">
                  </a>
                  {% else %}
                 <a class="logo_name" href="/">{{ shop.name }}</a>
                  {% endif %}
                  {% if template == 'index' %}
                  </h1>
                {% else %}
                </div>
              {% endif %}   
              {% if section.settings.top_promo_image_1 != blank %}
              <div class="grid__item wide--four-sixths post-large--three-sixths large--four-sixths small--hide promo_img">                
                {% if section.settings.top_promo_link_1 != blank %}
                <a href="{{ section.settings.top_promo_link_1 }}">
                {% endif %}
                <img src="{{ section.settings.top_promo_image_1 | img_url: 'master' }}" />
                {% if section.settings.top_promo_link_1 != blank %}
                </a>
                {% endif %}                              
              </div>
              {% endif %}
               </div>
              <ul class="menu_bar_right grid__item wide--two-sixths post-large--two-sixths">
                <li class="header-mobile">
              <div class="menu-block visible-phone"><!-- start Navigation Mobile  -->
                <div id="showLeftPush">
                  <i class="fa fa-bars" aria-hidden="true"> </i>
                </div>
              </div><!-- end Navigation Mobile  --> 
            </li> 
                 {% if section.settings.display_search %}                
                <li class="header-search large--hide medium-down--hide">
                  <div class="slidersearch">
                    <form action="/search" method="get" class="search-bar__table-cell search-bar__form" role="search">	
                      {% if settings.show_search_prt %} 
                      <input type="hidden" name="type" value="product">  
                      {% endif %}
                      <input type="text" id="search" name="q" value="{{ search.terms | escape }}" placeholder="{{ 'general.search.placeholder' | t }}" aria-label="{{ 'general.search.placeholder' | t }}" class="search-bar__input sb-search-input">
                      <button class="sb-search-submit" type="submit" value=""><i class="fa fa-search"></i></button>
                    </form>
                  </div>
                </li>
                {% endif %}
                
                 {% if section.settings.display_search %}  
                <li class="header-search wide--hide post-large--hide">
                  <div class="header_toggle"><span class="zmdi zmdi-search"></span></div>
                  <div class="slidersearch">
                    <form action="/search" method="get" class="search-bar__table-cell search-bar__form" role="search">
                      {% if settings.show_search_prt %} 
                      <input type="hidden" name="type" value="product">  
                      {% endif %}
                      <input type="text" id="search" name="q" value="{{ search.terms | escape }}" placeholder="{{ 'general.search.placeholder' | t }}" aria-label="{{ 'general.search.placeholder' | t }}" class="search-bar__input sb-search-input">
                      <button class="sb-search-submit res_btn" type="submit" value=""><i class="fa fa-search"></i></button>
                    </form>
                  </div>
                  </li>
                  {% endif %} 
                
                 {% if settings.display_currency %} 
                <li class="header_currency">
                  <ul class="tbl-list">
                    <li class="currency dropdown-parent uppercase currency-block">                       
                      {% include 'currencies-switcher' %}                      
                    </li>
                  </ul>
                </li> 
                {% endif %}
                 {% if section.settings.display_minicart %}
                <li class="header-bar__module cart header_cart">               
                  {% include 'mini-cart' %}                    
                </li> 
                {% endif %}
                
            

         

                
              </ul>
              
              </div>
              
              {% if section.settings.top_promo_image_1 != blank %}
              <div class="grid__item wide--hide post-large--hide large--hide medium--hide small--show promo_img">              
                {% if section.settings.top_promo_link_1 != blank %}
                <a href="{{ section.settings.top_promo_link_1 }}">
                {% endif %}
                <img src="{{ section.settings.top_promo_image_1 | img_url: 'master' }}" />
                {% if section.settings.top_promo_link_1 != blank %}
                </a>
                {% endif %}                                            
              </div>
              {% endif %}
               
      
  
     <style> 
        
   
     
     /* Logo block */
    .header-type-6 .site-header__menubar { background: {{ section.settings.logo_bar_bg }};}    
    .header-type-6 .site-header__logo a,.header-type-6 .header-mobile #showLeftPush { color:{{section.settings.logo_text }};}
    .header-type-6 .site-header__logo a:hover,.header-type-6 .header-mobile #showLeftPush:hover { color:{{section.settings.logo_hover_text }};}    
    
    /* Menu  block */
     .header-type-6 .menubar-section,.mobile-nav-section {background: {{ section.settings.menubar_bg }};}
    .header-type-6 .menu-tool ul li {color: {{ section.settings.menubar_text }};}
    .header-type-6 .menu-tool ul li a,.mobile-nav-section .mobile-nav-trigger {color:{{section.settings.menubar_links}};}  
    .header-type-6 .menu-tool ul li a:hover,.header-type-6 .menu-tool .site-nav > li > a.current:hover {color:{{section.settings.menubar_links_hover}};} 
    .header-type-6 .menu-tool .site-nav >  li > a.current {color:{{section.settings.menu_active}};} 
    .header-type-6 .site-nav-dropdown,#MobileNav,.mobile-nav__sublist { background: {{ section.settings.menubar_dropdown_bg }};}
    .header-type-6 .site-nav-dropdown .inner > a {color: {{ section.settings.menubar_dropdown_caption }};}    
    .header-type-6 .site-nav-dropdown .inner > a:hover {color: {{ section.settings.menubar_dropdown_caption_hover }};}    
    .header-type-6 .site-nav-dropdown .inner .dropdown a,.header-type-6 .menu-tool .site-nav .site-nav-dropdown li a,.header-type-6 .site-nav .widget-featured-product .product-title,.header-type-6 .site-nav .widget-featured-product .widget-title h3,#MobileNav a,.mobile-nav__sublist a,.site-nav .widget-featured-nav .owl-prev a,.site-nav .widget-featured-nav .owl-next a  {color: {{ section.settings.menubar_dropdown_links }};}
    .header-type-6 .site-nav-dropdown .inner .dropdown a:hover,.header-type-6 .menu-tool .site-nav .site-nav-dropdown li a:hover,.header-type-6 .site-nav-dropdown .inner .dropdown a.current,.header-type-6 .menu-tool .site-nav .site-nav-dropdown li a.current,.header-type-6 .site-nav .widget-featured-product .product-title:hover,#MobileNav a.current,.mobile-nav__sublist a.current,.site-nav .widget-featured-nav .owl-prev a:hover,.site-nav .widget-featured-nav .owl-next a:hover {color: {{ section.settings.menubar_dropdown_links_hover }};}    
    
    /* Dropdown block */
    .header-type-6 .menubar-section #Togglemodal i {color: {{ section.settings.dropdown_icon }};}
    .header-type-6 .menubar-section #Togglemodal i:hover {color: {{ section.settings.dropdown_icon_hover }};}
    .header-type-6 #slidedown-modal {background: {{ section.settings.dropdown_bg }};}
    .header-type-6 #slidedown-modal ul li a {color:{{section.settings.dropdown_link}};} 
    .header-type-6 #slidedown-modal ul li a:hover {color:{{section.settings.dropdown_hover_link}};} 
    
    
    /* Search block */     
     .header-type-6 .header-search input#search {color:{{section.settings.search_text }};} 
     .header-type-6 .header-search span, .header-type-6 .header-search .res_btn  {color:{{section.settings.search_icon}};background:none;} 
     .header-type-6 .header-search span:hover, .header-type-6 .header-search .res_btn:hover {color:{{section.settings.search_icon_hover }};background:none;} 
     .header-type-6 .search-bar__form  { border:1px solid {{ section.settings.border_between_bars}}; background:{{section.settings.search_bg }};}
     .header-type-6 .header-search button { color:{{section.settings.search_btn_text }};background:{{ section.settings.search_btn_bg }};} 
     .header-type-6 .header-search button:hover { color:{{section.settings.search_btn_text_hvr}};background:{{ section.settings.search_bg_hover }};} 
       
       .header-type-6 .header-search input#search::-webkit-input-placeholder  { /* Chrome/Opera/Safari */
       color:{{section.settings.search_text }};
     }
     .header-type-6 .header-search input#search::-moz-placeholder { /* Firefox 19+ */
       color:{{section.settings.search_text }};
     }
     .header-type-6 .header-search input#search:-ms-input-placeholder { /* IE 10+ */
       color:{{section.settings.search_text }};
     }
     .header-type-6 .header-search input#search:-moz-placeholder { /* Firefox 18- */
       color:{{section.settings.search_text }};
     }
     
     /* Cart Summary block */
       .header-type-6 #cartCount  {color: {{ section.settings.minicart_icon }};}
    .header-type-6 #cartCount:hover  {color: {{ section.settings.minicart_icon_hover }};}
   
    .header-type-6 #slidedown-cart .actions, .header-type-6  #slidedown-cart  {background: {{ section.settings.minicart_bg }};}
    .header-type-6 .header-bar__module p {color: {{ section.settings.minicart_text }};}
    .header-type-6 .header-bar__module a {color:{{section.settings.minicart_link }};}  
    .header-type-6 .header-bar__module a:hover {color:{{section.settings.minicart_link_hover}};} 
    .header-type-6 .header-bar__module .btn {color:{{section.settings.minicart_btn_text}};background: {{ section.settings.minicart_btn_bg }};} 
    .header-type-6 .header-bar__module .btn:hover {color:{{section.settings.minicart_btn_hover_text}};background: {{ section.settings.minicart_btn_hover_bg }};} 
    .header-type-6  #slidedown-cart .total .price, .header-type-6 #minicart_total,.header-type-6 #slidedown-cart ul li .cart-collateral {color:{{section.settings.minicart_total }};} 
    .header-type-6 #slidedown-cart li { border-bottom:1px solid {{ section.settings.border_between_bars}}; }

    
     /* Currency block */
    
  

       .header-type-6 .header_currency ul select,.header-type-6 .header_currency ul li.currency .selector-arrow::after {color:{{section.settings.currency_icon}};}   
       .header-type-6 .header_currency ul select:hover {color:{{section.settings.currency_icon_hover}};}  
       .header-type-6 .header_currency ul li.currency:hover:after {border-top-color:{{section.settings.currency_icon_hover}};}
       .header-type-6 .header_currency ul li.currency:after {border-top-color:{{section.settings.currency_icon}};}
       .header-type-6 .header_currency option {background:{{section.settings.currency_bg}};color:{{section.settings.currency_link}};}


      
       /* Header borders */
       {% if section.settings.clr_border_image %}   .header-type-6 .site-header__menubar:after,.header-type-6 .is-sticky .menubar-section:after { background-image:url({{ section.settings.border_image | img_url: 'master' }});background-repeat:no-repeat;content:"";float:left;width:100%;height:3px; } {% endif %}
  	.header-type-6 .top_bar li a,.header-type-6 .top_bar li span { border-right:1px solid {{ section.settings.top_border_between_bars }}; }
    .header-type-6 .top_bar ul li:last-child,.header-type-6 .top_bar li:last-child span,.header-type-6 .top_bar li:last-child a { border-right:none; }
       
        @media (max-width: 767px) {
       
      .header-type-6 .header-mobile { width:auto;float:none;display:inline-block; }
          
        }
           
      </style> 


